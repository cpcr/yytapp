  <select id="findAll" resultMap="RulePushResultMap">
     SELECT
       <include refid="Base_Column_List" /> , 'wechat' PLATFORM_TYPE
    from SYS_RULE_PUSH_WECHAT
    union all
    SELECT
        <include refid="Base_Column_List" /> , 'alipay' PLATFORM_TYPE
    from SYS_RULE_PUSH_ALIPAY
    union all 
    SELECT
        <include refid="Base_Column_List" /> , 'easyHealth' PLATFORM_TYPE
    from SYS_RULE_PUSH_EASYHEALTH
  </select>
  
